pipeline{
    agent any
    tools{
     maven 'M2_HOME'
   }
    stages{
        stage('sonarqube scan'){
            steps{
                sh 'mvn sonar:sonar'
            }
        }
        stage('maven commandes'){
            steps{
                sh 'mvn clean'
                sh 'mvn validate'
                sh 'mvn compile'
                sh 'mvn install'
                sh 'mvn test'
                sh 'mvn package'
            }
        }
    }
}